@model App.Web.ViewModels.User.UserAddOrEditVM

@{
	ViewData["Title"] = "Tạo tài khoản";
	ViewBag.NavBarCodePage = NavBarCodePage.USER_LIST;
}
@section head{
	<link href="~/lib/bs4-autocomplete/css/bs4-autocomplete.css" rel="stylesheet" />
}
<div class="row">
	<div class="col-12">
		<form asp-action="Create" autocomplete="off">
			<div asp-validation-summary="ModelOnly" class="text-danger"></div>
			<partial name="_AddOrEdit" />
			<partial name="Form/_FormAction" />
		</form>
	</div>
</div>
<partial name="Form/_BackToList" />
@section scripts{
	<script src="~/js/user/create-edit-user.js" asp-append-version="true"></script>
}